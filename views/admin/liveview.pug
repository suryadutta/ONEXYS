extends ../layout

block content
    script.
        const herokuAPI = "/api";
    br
    .container
        .row
            .form.form-inline
                h2='Module Live View'
    hr
    br
    .container
        .row
            table(class="admin_table")
                tr
                    th ID
                    th Primary Title
                    th Secondary Title
                    th Open?
                    th Practice Link
                    th Quiz Link
                    th Reflections Link
                    th Preview
                each module, i in modules
                    tr
                        td=module._id
                        td=module.primary_title
                        td=module.secondary_title
                        td=module.open
                        td=module.practice_link
                        td=module.quiz_link
                        td=module.reflection_link
                        td
                            a.btn.btn-primary(href=`/modules/${i+1}`, target="_blank") Live View
