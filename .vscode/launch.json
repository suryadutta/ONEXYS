  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Nodemon",
      "processId": "${command:PickProcess}",
      "restart": true,
      "port": 8001,
      "protocol": "inspector"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Heroku",
      "address": "localhost",
      "port": 8080,
      "protocol": "inspector",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/app"
    }
  ]
}
